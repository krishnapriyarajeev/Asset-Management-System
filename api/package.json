{
  "name": "api",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "test": "rm -rf dist/ && jest",
    "test:coverage": "jest --coverage",
    "build": "rm -rf dist/ && tsc",
    "start": "npm run build && node dist/src/app.js",
    "migration:create": "npm run build && npx typeorm migration:create",
    "migration:generate": "npm run build && npx typeorm migration:generate -d dist/src/db/data-source.db.js",
    "migration:run": "npm run build && npx typeorm migration:run -d dist/src/db/data-source.db.js",
    "migration:revert": "npm run build && npx typeorm migration:revert -d dist/src/db/data-source.db.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/typeorm": "^10.0.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "jest-when": "^3.6.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/exceljs": "^1.3.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jest-when": "^3.5.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "jest": {
    "transform": {
      "^.+\\.(t)s$": "ts-jest"
    },
    "verbose": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts",
      "!**/index.ts"
    ]
  },
  "directories": {
    "test": "test"
  },
  "description": ""
}
